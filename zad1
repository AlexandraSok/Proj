public class Main {
    public static void main(String[] args) {
        int licznik = 1;
        while(licznik<50){
  if(licznik%3==0){
    System.out.println( licznik + " foo " );}
   else if (licznik%5==0){
    System.out.println( licznik + " bar " );}
  else if (licznik%7==0){
    System.out.println( licznik + " baz " );}
else{
  System.out.println( licznik );
}
		  licznik++;
	  }
    }
}
