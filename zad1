public class Main {
    public static void main(String[] args) {
        while(licznik<51){
  if(licznik%3==0){
    System.out.println( licznik + " foo " )};
   elif (licznik%5==0){
    System.out.println( licznik + " bar " )};
  elif (licznik%7==0){
    System.out.println( licznik + " baz " )};
else{
  System.out.println( licznik )
}
		  licznik++;
	  }
    }
}
